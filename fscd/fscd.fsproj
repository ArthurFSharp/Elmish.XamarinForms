      <HintPath>..\..\..\packages\neutral\FSharp.Core\lib\net45\FSharp.Core.dll</HintPath>
    </Reference>
    <!-- <PackageReference Update="FSharp.Core" Version="4.5.0" /> -->
  </ItemGroup>

</Project>
